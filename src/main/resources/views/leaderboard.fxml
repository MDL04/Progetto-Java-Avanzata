<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LeaderboardController">

    <Button alignment="CENTER" layoutX="241.0" layoutY="346.0" onAction="#goToHome" text="Torna alla home" />
   <TableView fx:id="leaderboardTable" layoutX="50.0" layoutY="28.0" prefHeight="300.0" prefWidth="500.0">
     <columns>
        <TableColumn fx:id="usernameColumn" prefWidth="75.0" text="Username" />
        <TableColumn fx:id="easyColumn" prefWidth="75.0" text="Facile" />
        <TableColumn fx:id="mediumColumn" prefWidth="75.0" text="Medio" />
        <TableColumn fx:id="hardColumn" prefWidth="75.0" text="Difficile" />
     </columns>
      <columnResizePolicy>
         <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      </columnResizePolicy>
   </TableView>

</AnchorPane>
